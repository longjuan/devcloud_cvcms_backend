<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cvc.cvcms.dao.RoleDao">
    <select id="getRoleByRoleName" resultType="role">
        select
             *
        from
             role
        where
             role_name = #{roleName} ;
    </select>
</mapper>